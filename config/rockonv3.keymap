#include "keys_hu.h"
#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        layer_0 {
            bindings = <
&kp N1  &kp N2  &kp N3
&kp N4  &kp N5  &kp N6
&kp N7  &kp N8  &mo 1
            >;
        };

        bt_layer {
            bindings = <
&bt BT_SEL 0  &bt BT_SEL 1  &bt BT_SEL 2
&bt BT_CLR    &trans        &trans
&bootloader   &trans        &trans
            >;
        };
    };
};
